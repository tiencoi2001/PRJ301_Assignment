<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.auth.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowingController</servlet-name>
        <servlet-class>Controller.flim.ShowingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>Controller.auth.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ComingController</servlet-name>
        <servlet-class>Controller.flim.ComingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>Controller.flim.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailController</servlet-name>
        <servlet-class>Controller.flim.DetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>Controller.auth.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashbroadController</servlet-name>
        <servlet-class>Controller.user.DashbroadController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TransactionsHistoryController</servlet-name>
        <servlet-class>Controller.user.TransactionsHistoryController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountDetailController</servlet-name>
        <servlet-class>Controller.user.AccountDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetDateController</servlet-name>
        <servlet-class>Controller.booking.GetDateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetTimeController</servlet-name>
        <servlet-class>Controller.booking.GetTimeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetChairController</servlet-name>
        <servlet-class>Controller.booking.GetChairController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentController</servlet-name>
        <servlet-class>Controller.booking.PaymentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowingController</servlet-name>
        <url-pattern>/showing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ComingController</servlet-name>
        <url-pattern>/coming</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailController</servlet-name>
        <url-pattern>/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashbroadController</servlet-name>
        <url-pattern>/dashbroad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransactionsHistoryController</servlet-name>
        <url-pattern>/transactionsHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountDetailController</servlet-name>
        <url-pattern>/accountDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetDateController</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetTimeController</servlet-name>
        <url-pattern>/booking_nextstep</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetChairController</servlet-name>
        <url-pattern>/booking_laststep</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentController</servlet-name>
        <url-pattern>/payment</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
